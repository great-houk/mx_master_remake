{
    "version": "0.2.0",
    "configurations": [
        {
            "preLaunchTask": "buildDebug",
            "type": "probe-rs-debug",
            "request": "launch",
            "name": "Run (Debug)",
            "cwd": "${workspaceFolder}",
            "speed": 2000,
            "runtimeExecutable": "probe-rs",
            "runtimeArgs": [
                "dap-server"
            ],
            "chip": "nRF52840_xxAA",
            "flashingConfig": {
                "flashingEnabled": true,
                "haltAfterReset": false,
            },
            "coreConfigs": [
                {
                    "coreIndex": 0,
                    "programBinary": "${workspaceFolder}/target/thumbv7em-none-eabihf/debug/mx-master-remake",
                    "svdFile": "${workspaceFolder}/nrf52840.svd",
                    "rttEnabled": true,
                    "rttChannelFormats": [
                        {
                            "channelNumber": 0,
                            "dataFormat": "String",
                            "showTimestamps": true
                        },
                    ]
                }
            ],
            "env": {
                // If you set this variable, check the VSCode console log window for the location of the log file.
                "RUST_LOG": "info",
                "DEFMT_LOG": "info",
            },
            // Info, Debug
            "consoleLogLevel": "Console"
        },
        {
            "preLaunchTask": "buildRelease",
            "type": "probe-rs-debug",
            "request": "launch",
            "name": "Run (Release)",
            "cwd": "${workspaceFolder}",
            "speed": 2000,
            "runtimeExecutable": "probe-rs",
            "runtimeArgs": [
                "dap-server"
            ],
            "chip": "nRF52840_xxAA",
            "flashingConfig": {
                "flashingEnabled": true,
                "haltAfterReset": false,
            },
            "coreConfigs": [
                {
                    "coreIndex": 0,
                    "programBinary": "${workspaceFolder}/target/thumbv7em-none-eabihf/debug/mx-master-remake",
                    "svdFile": "${workspaceFolder}/nrf52840.svd",
                    "rttEnabled": true,
                    "rttConfig": {
                        "upChannel": "Terminal",
                        "downChannel": "Terminal",
                    },
                    "rttChannelFormats": [
                        {
                            "channelNumber": 0,
                            "dataFormat": "String",
                        },
                    ]
                }
            ],
            "env": {
                // If you set this variable, check the VSCode console log window for the location of the log file.
                "RUST_LOG": "info",
                "DEFMT_LOG": "info",
            },
            // Info, Debug
            "consoleLogLevel": "Console"
        }
    ]
}